<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sanitation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/vick3"
        android:label="SaniApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".navigate_down"></activity>
        <activity android:name=".viewlist" />
        <activity android:name=".status" />
        <activity android:name=".tracks" />
        <activity android:name=".menulist" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>